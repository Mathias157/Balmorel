VHYDROGEN_STOLOADT.UP(IY411,IA,IHYDROGEN_GH2STO,S,T)$(GKFX(IY411,IA,IHYDROGEN_GH2STO) AND (NOT SUM(Y$(YVALUE(Y) LE YVALUE(IY411)),IAGKNY(Y,IA,IHYDROGEN_GH2STO))) AND (NOT (IGDECOMEXOPOT(IY411,IA,IHYDROGEN_GH2STO))))= GKFX(IY411,IA,IHYDROGEN_GH2STO)*(1$(NOT IGKRATE(IA,IHYDROGEN_GH2STO,S,T)) + IGKRATE(IA,IHYDROGEN_GH2STO,S,T))/GDATA(IHYDROGEN_GH2STO,'GDSTOHLOAD');
VHYDROGEN_GH2_T.UP(IY411,IA,IHYDROGEN_GH2STO,S,T)$(GKFX(IY411,IA,IHYDROGEN_GH2STO) AND (NOT SUM(Y$(YVALUE(Y) LE YVALUE(IY411)),IAGKNY(Y,IA,IHYDROGEN_GH2STO))) AND (NOT (IGDECOMEXOPOT(IY411,IA,IHYDROGEN_GH2STO))))= GKFX(IY411,IA,IHYDROGEN_GH2STO)*(1$(NOT IGKRATE(IA,IHYDROGEN_GH2STO,S,T)) + IGKRATE(IA,IHYDROGEN_GH2STO,S,T))/GDATA(IHYDROGEN_GH2STO,'GDSTOHUNLD');

$ifi not %COMBTECH%==yes $goto NO_COMBTECH
*Not allowing production from biomethane units if combtech technologies producing biomethane do not have potential production (exist or possibly invested)
VGF_T.FX(IY411,IA,IGBIOMETHANE,S,T)$(IAGK_HASORPOT(IY411,IA,IGBIOMETHANE) AND (NOT(SUM((IAI,IHYDROGEN_GH2TOBIOMETH),IAGK_HASORPOT(IY411,IAI,IHYDROGEN_GH2TOBIOMETH))) AND NOT(SUM((IAI,IHYDROGEN_GBIOGASUPGRADING),IAGK_HASORPOT(IY411,IAI,IHYDROGEN_GBIOGASUPGRADING))) AND NOT(SUM((IAI,IHYDROGEN_GBIOGASMETHANATION),IAGK_HASORPOT(IY411,IAI,IHYDROGEN_GBIOGASMETHANATION))))) = 0;
VGE_T.FX(IY411,IA,IGBIOMETHANE,S,T)$(IAGK_HASORPOT(IY411,IA,IGBIOMETHANE) AND (NOT(SUM((IAI,IHYDROGEN_GH2TOBIOMETH),IAGK_HASORPOT(IY411,IAI,IHYDROGEN_GH2TOBIOMETH))) AND NOT(SUM((IAI,IHYDROGEN_GBIOGASUPGRADING),IAGK_HASORPOT(IY411,IAI,IHYDROGEN_GBIOGASUPGRADING))) AND NOT(SUM((IAI,IHYDROGEN_GBIOGASMETHANATION),IAGK_HASORPOT(IY411,IAI,IHYDROGEN_GBIOGASMETHANATION)))) AND IGE(IGBIOMETHANE)) = 0;
VGH_T.FX(IY411,IA,IGBIOMETHANE,S,T)$(IAGK_HASORPOT(IY411,IA,IGBIOMETHANE) AND (NOT(SUM((IAI,IHYDROGEN_GH2TOBIOMETH),IAGK_HASORPOT(IY411,IAI,IHYDROGEN_GH2TOBIOMETH))) AND NOT(SUM((IAI,IHYDROGEN_GBIOGASUPGRADING),IAGK_HASORPOT(IY411,IAI,IHYDROGEN_GBIOGASUPGRADING))) AND NOT(SUM((IAI,IHYDROGEN_GBIOGASMETHANATION),IAGK_HASORPOT(IY411,IAI,IHYDROGEN_GBIOGASMETHANATION)))) AND IGH(IGBIOMETHANE)) = 0;
$label NO_COMBTECH

* Complement to equation QXH2K_UP for the case with no endogenous transmission investments.
$ifi %h2_transmission_definition%=='region' VXH2_T.UP(IY411,IRE,IRI,S,T)$(IXH2K_HASORPOT(IY411,IRE,IRI) AND (NOT SUM(Y$(YVALUE(Y) LE YVALUE(IY411)),IXH2KN(IY411,IRE,IRI))) AND (NOT SUM(Y$(YVALUE(Y) LE YVALUE(IY411)),IXH2KN_NGTOH2(IY411,IRE,IRI)))) = XH2KFX(IY411,IRE,IRI)*(1$(NOT IXH2KRATE(IRE,IRI,S,T))+IXH2KRATE(IRE,IRI,S,T)) ;
$ifi %h2_transmission_definition%=='area'   VXH2_T.UP(IY411,IAE,IAI,S,T)$(IXH2K_HASORPOT(IY411,IAE,IAI) AND (NOT SUM(Y$(YVALUE(Y) LE YVALUE(IY411)),IXH2KN(IY411,IAE,IAI))) AND (NOT SUM(Y$(YVALUE(Y) LE YVALUE(IY411)),IXH2KN_NGTOH2(IY411,IAE,IAI)))) = XH2KFX(IY411,IAE,IAI)*(1$(NOT IXH2KRATE(IAE,IAI,S,T))+IXH2KRATE(IAE,IAI,S,T)) ;



