VGBIOGASMETHANATION_T.UP(IY411,IA,IGDISPATCH(IHYDROGEN_GBIOGASMETHANATION),S,T)$(GKFX(IY411,IA,IHYDROGEN_GBIOGASMETHANATION) AND (NOT SUM(Y$(YVALUE(Y) LE YVALUE(IY411)),IAGKNY(Y,IA,IHYDROGEN_GBIOGASMETHANATION))) AND (NOT (IGDECOMEXOPOT(IY411,IA,IHYDROGEN_GBIOGASMETHANATION)))) = GKFX(IY411,IA,IHYDROGEN_GBIOGASMETHANATION)*(1$(NOT IGKRATE(IA,IHYDROGEN_GBIOGASMETHANATION,S,T)) + IGKRATE(IA,IHYDROGEN_GBIOGASMETHANATION,S,T));
